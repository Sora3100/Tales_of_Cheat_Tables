alloc(newmem,$1000)

label(code)
label(return)

newmem:
  mov [tbl_data],esi
code:
  mov edx,[esi+00009340]
  jmp return

btl_char_data:
  jmp newmem
  nop
return:
registersymbol(btl_char_data)

