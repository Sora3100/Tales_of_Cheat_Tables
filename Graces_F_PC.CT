                  <AssemblerScript>[ENABLE]
{$lua}
if syntaxcheck then return end

local script_name = "encounter_editor"

script_tbl[memrec.ID] = script_name

local currEncounter
local currEnemyCount = 0
local base_enc_file
local offset_to_section2
local offset_to_enemy_data
local enemy_data_ptr
local encounterMultiplierMemRec = getAddressList().getMemoryRecordByID("4340")

_G[script_name] = function()
   if not (getAddressSafe("encounter_file") == nil) then
      --update pointer to enemy data
      if not (readInteger("tbl_data+8") == currEncounter) then
         currEncounter = readInteger("tbl_data+8")

         base_enc_file = getAddress("encounter_file")
         offset_to_section2 = swapEndian(readInteger("encounter_file+0xC"))
         offset_to_enemy_data = swapEndianWord(readSmallInteger(getAddress("encounter_file") + 0x14 + 0x6 + (0x8 * currEncounter))) * 0x4

         enemy_data_ptr = base_enc_file + offset_to_section2 + offset_to_enemy_data
         writeQword("tbl_data+10", enemy_data_ptr)
      end

      local enemyCount = readBytes(getAddress("encounter_file+14+8*[tbl_data+8]"), 1, false)

      --generate enemy memory records
      if not (enemyCount == currEnemyCount) then
         if (encounterMultiplierMemRec.Count &gt; enemyCount) then
            for i = encounterMultiplierMemRec.Count, enemyCount + 1, -1 do
                encounterMultiplierMemRec.Child[i - 1].Destroy()
            end
         elseif (encounterMultiplierMemRec.Count &lt; enemyCount) then
                for i = encounterMultiplierMemRec.Count + 1, enemyCount, 1 do
                    if (i &gt; 100) then
                       break
                    end

                    local enemyHeaderMemRec = getAddressList().createMemoryRecord()

                    enemyHeaderMemRec.Type = vtCustom
                    enemyHeaderMemRec.CustomTypeName = "2 Byte Big Endian"
                    enemyHeaderMemRec.Description = "Enemy "..i
                    enemyHeaderMemRec.Address = "tbl_data+10"
                    enemyHeaderMemRec.OffsetCount = 1
                    enemyHeaderMemRec.Offset[0] = 0x4 * (i - 1)
                    enemyHeaderMemRec.DropDownLinkedMemRec = "dropdown: Enemy List -1"
                    enemyHeaderMemRec.DropDownLinked = true
                    enemyHeaderMemRec.Options = "[moAllowManualCollapseAndExpand, moManualExpandCollapse]"
                    enemyHeaderMemRec.DontSave = true
                    enemyHeaderMemRec.appendToEntry(encounterMultiplierMemRec)

                    local enemyPosMemRec = getAddressList().createMemoryRecord()

                    enemyPosMemRec.Type = vtCustom
                    enemyPosMemRec.CustomTypeName = "2 Byte Big Endian"
                    enemyPosMemRec.Description = "Position"
                    enemyPosMemRec.Address = "tbl_data+10"
                    enemyPosMemRec.OffsetCount = 1
                    enemyPosMemRec.Offset[0] = 0x4 * (i - 1) + 0x2
                    enemyPosMemRec.DontSave = true
                    enemyPosMemRec.appendToEntry(enemyHeaderMemRec)
                end
         end

         currEnemyCount = enemyCount
      end
   end
end
