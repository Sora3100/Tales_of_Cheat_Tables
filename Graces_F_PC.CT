function toHex(str)
  return string.upper(string.format("%X", str))
end

function swapEndianWord(n)
    local b1 = n % 256
    n = (n - b1) / 256
    local b2 = n % 256
    n = (n - b2) / 256
    return b1 * 0x100 + b2
end

function swapEndian(n)
             local b1 = n % 256
             n = (n - b1) / 256
             local b2 = n % 256
             n = (n - b2) / 256
             local b3 = n % 256
             n = (n - b3) / 256
             local b4 = n % 256
             n = (n - b4) / 256
             return b1 * 0x1000000 + b2 * 0x10000 + b3 * 0x100 + b4
end

