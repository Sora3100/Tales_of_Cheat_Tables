          <ID>91395</ID>
          <Description>"Weapons/Armor/Gem Editor"</Description>
          <Options moHideChildren="1"/>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>[ENABLE]
{$lua}
if syntaxcheck then return end

local function mainloop()

      local equipPageHover = readSmallInteger("baseElfAddr+3B9138")
      --local itemPageHover = readSmallInteger("baseElfAddr+3BC1F8")
      local enhancePageHover = readSmallInteger("baseElfAddr+3BC1F8")

      --Hover check
      if not (readInteger("gemNum+8") == 0) then
           if not (readInteger("baseElfAddr") == nil) then
                 --equip page hover
                 if (equipPageHover &gt; 511) and (equipPageHover &lt; 3005) then
                        itemType = readByte("baseElfAddr+3B9139")
                        --print(1)
                        itemCheck(equipPageHover)

                 --doesn't stay in memory so ignore
                 --item page hover
                 --elseif (itemPageHover &gt; 511) and (itemPageHover &lt; 3005) then
                 --       itemType = readByte("baseElfAddr+3BC1F9")
                 --       --print(2)
                 --       itemCheck(itemPageHover)

                 --enhance page hover
                 elseif (enhancePageHover &gt; 511) and (enhancePageHover &lt; 3005) then
                        itemType = readByte("baseElfAddr+3BC1F9")
                        --print(3)
                        itemCheck(enhancePageHover)
                 end
           end
      end
end

function itemCheck(item)
         if not (item == 0) and not (item == nil) then

            if (item &lt; 512) or (item &gt; 1084) then
               return
            else
                if (itemType == 2) then
                   item = item - 512
                elseif (itemType == 3) then
                   item = item - 768
                elseif (itemType == 4) then
                   item = item - 1024
                end
            end

            writeInteger(getAddress("gemNum"), item)
            else
               return
            end
end

if not timer then
   timer = createTimer(nil)
   timer.OnTimer = function(timer)
                   mainloop()
                   end
   timer.Interval = 1000
   timer.Enabled = true
else
   timer.Enabled = true
end
{$asm}
globalalloc(gemNum,8)

gemNum+8:
  dq 1

[DISABLE]
{$lua}
if syntaxcheck then return end

if (timer) then
   timer.Enabled = false
   timer.Destroy()
   timer = nil
end
{$asm}
dealloc(gemNum)
unregistersymbol(gemNum)
